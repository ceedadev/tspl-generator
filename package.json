{
  "name": "tspl-generator",
  "version": "1.0.0",
  "description": "A TypeScript library for generating TSPL/TSPL2 commands for thermal label printers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/browser/tspl-generator.min.js",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:browser": "webpack",
    "build:all": "npm run build && npm run build:browser",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build:all",
    "example:basic": "ts-node examples/basic-label.ts",
    "example:advanced": "ts-node examples/advanced-label.ts",
    "example:browser": "node examples/serve.js"
  },
  "keywords": [
    "tspl",
    "tspl2",
    "thermal",
    "printer",
    "label",
    "barcode",
    "qrcode"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
